class BuzzNumber
{
 
 public void main()
 {
 Scanner sc=new Scanner(System.in);
  System.out.print("Enter the number:");
 int  a = sc.nextInt();
  if( a % 10 == 7 || a % 7 == 0 )
   System.out.println("Entered number is a Buzz number.");
  else
   System.out.println("Entered number is not a Buzz number.");

 }
 }
